{
  "name": "zendesk_app_template",
  "version": "1.0.0",
  "description": "A template for Zendesk apps based on the Zendesk App Scaffold",
  "keywords": [
    "zendesk",
    "app",
    "framework"
  ],
  "author": "Alex Culligan",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aculligan/zendesk_app_template"
  },
  "scripts": {
    "build:dev": "webpack --env.development",
    "build": "webpack --env.production -p",
    "create": "zat create -p ./dist",
    "lint": "standard",
    "package": "zat package -p ./dist",
    "start": "zat server -p ./dist",
    "stats": "node ./webpack/stats.js",
    "test": "zat validate -p ./dist",
    "update": "zat update -p ./dist",
    "validate": "zat validate -p ./dist",
    "watch": "webpack --watch --env.development"
  },
  "devDependencies": {
    "standard": "^13.0.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@zendeskgarden/css-arrows": "^3.1.1",
    "@zendeskgarden/css-avatars": "^6.0.1",
    "@zendeskgarden/css-bedrock": "^7.0.26",
    "@zendeskgarden/css-buttons": "^7.0.10",
    "@zendeskgarden/css-callouts": "^3.3.8",
    "@zendeskgarden/css-forms": "^7.0.11",
    "@zendeskgarden/css-grid": "^0.1.29",
    "@zendeskgarden/css-menus": "^9.0.11",
    "@zendeskgarden/css-modals": "^6.4.12",
    "@zendeskgarden/css-pagination": "^4.0.10",
    "@zendeskgarden/css-tables": "^4.0.10",
    "@zendeskgarden/css-tabs": "^6.0.10",
    "@zendeskgarden/css-tags": "^5.1.6",
    "@zendeskgarden/css-tooltips": "^4.1.18",
    "@zendeskgarden/css-utilities": "^4.3.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "handlebars-loader": "^1.7.1",
    "handlebars": "^4.1.2",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "regenerator-runtime": "^0.13.2",
    "underscore": "^1.9.1",
    "webpack-cli": "^3.3.5",
    "webpack": "^4.35.3"
  }
}
